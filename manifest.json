{
  "manifest_version": 2,

  "name": "Vocab Extension",
  "description": "Get yo score up",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click Here!"
  },
  "chrome_url_overrides":{
    "newtab": "newtab.html"

  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]

}
 